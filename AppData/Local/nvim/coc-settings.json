{
	"diagnostic.errorSign": "",
	"diagnostic.warningSign": "",
	"diagnostic.infoSign": "",
	"diagnostic.hintSign": "",
	"languageserver": {
		"rust-analyzer": {
			"updates": {
				"prompt": "true",
				"channel": "nightly"
			},
			"cargo.allFeatures": "true",
			"checkOnSave.command": "clippy",
			"filetypes": ["rs"],
			"rustfmt.enableRangeFormatting": "true"
		}
	}
}
